{
  "name": "dmftaras/amqp-queue",
  "description": "AMQP Queue Yii 2 extension",
  "type": "yii2-extension",
  "keywords": ["yii2","amqp","queue", "extension"],
  "license": "MIT",
  "authors": [
    {
      "name": "Taras Kostetskyi",
      "email": "dmftaras@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2.0",
    "ext-json": "*",
    "yiisoft/yii2": ">=2.0.51",
    "php-amqplib/php-amqplib" : "^3.7"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "dmftaras\\amqp_queue\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
